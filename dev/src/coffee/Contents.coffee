Bg = require('./Bg');


# ---------------------------------------------------
# コンテンツ
# ---------------------------------------------------
class Contents
  
  
  # -----------------------------------------------
  # コンストラクタ
  # -----------------------------------------------
  constructor: ->
    
    
    @_bg;
  
  
  
  # -----------------------------------------------
  # 初期化
  # -----------------------------------------------
  init: =>
    
    @_start();
  
  
  
  # -----------------------------------------------
  # コンテンツスタート
  # -----------------------------------------------
  _start: =>
    
    @_bg = new Bg("xBg");
    @_bg.init();
    
    $("#xBg").css({
      position:"absolute",
      top:0,
      left:0
    });









module.exports = Contents;